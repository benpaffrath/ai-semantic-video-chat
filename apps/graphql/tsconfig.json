{
    "compilerOptions": {
      "target": "ES2022",        
      "lib": ["ES2022", "DOM"],
      "module": "ESNext",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,      
      "strict": true,
      "outDir": "dist",
    }
  }
  